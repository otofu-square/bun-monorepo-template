{
  "name": "@bun-monorepo-template/server",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "imports": {
    "#src/*": "./src/*.ts"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "typecheck": "tsgo"
  },
  "dependencies": {
    "@bun-monorepo-template/shared": "workspace:*"
  }
}
